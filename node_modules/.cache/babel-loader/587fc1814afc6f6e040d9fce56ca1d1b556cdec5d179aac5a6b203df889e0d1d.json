thorization: `Bearer ${token}`\n          }\n        });\n        const data = await res.json();\n        if (res.ok) {\n          this.userId = data.users_id;\n          this.fetchJobsByUser();\n        } else {\n          console.error(\"Unable to get user ID:\", data.message);\n        }\n      } catch (err) {\n        console.error(\"Error getting user ID:\", err);\n      }\n    }\n  }\n};","map":{"version":3,"names":["IndexComponent","globalVariable","name","components","data","jobs","searchQuery","currentPage","perPage","userEmail","userId","showModal","selectedAboutId","computed","isAdmin","toLowerCase","filteredAgents","query","filter","agent","first_name","last_name","province","district","includes","paginatedAgents","start","end","slice","totalPages","Math","ceil","length","watch","mounted","localStorage","getItem","getUserIdFromEmail","methods","changePage","page","fetchJobsByUser","token","res","fetch","headers","Authorization","ok","Error","json","err","console","error","goToEdit","users_id","$router","push","params","id","goToMore","confirmDelete","closeModal","deleteAbout","method","job","message","payload","JSON","parse","atob","split","email"],"sources":["/Users/gentil/Desktop/Kozi-ai-agent/src/views/admin/ViewAgents.vue"],"sourcesContent":["<template>\n  <IndexComponent />\n  <div\n    class=\"page-wrapper\"\n    id=\"main-wrapper\"\n    data-layout=\"vertical\"\n    data-navbarbg=\"skin6\"\n    data-sidebartype=\"full\"\n    data-sidebar-description=\"fixed\"\n    data-header-description=\"fixed\"\n    style=\"position: absolute; top: 0; left: 0; right: 0; bottom: 0; z-index: 1;\"\n  >\n    <div class=\"body-wrapper\" style=\"margin-top: 70px; margin-left: 270px; background: white; min-height: calc(100vh - 70px); padding: 20px; overflow-y: auto; max-height: calc(100vh - 70px);\">\n      <h5 class=\"card-title fw-semibold mb-4\" style=\"color: #E960A6;\">\n        <i class=\"ti ti-user-plus\"></i> View agent\n      </h5>\n\n      <!-- Search -->\n      <div class=\"d-flex justify-content-center mb-3\">\n        <input\n          v-model=\"searchQuery\"\n          type=\"text\"\n          class=\"form-control\"\n          placeholder=\"Search by names, province or district\"\n          style=\"max-width: 450px;\"\n        />\n      </div>\n      \n<div v-if=\"filteredAgents.length === 0\" class=\"d-flex justify-content-center my-4\">\n        <div class=\"alert alert-danger text-center\" style=\"width: 40rem;\">\n          No agent found.\n        </div>\n      </div>\n\n<!-- Agent Table -->\n<div class=\"table-responsive\" v-else>\n  <table class=\"table table-bordered\">\n    <thead class=\"table-light\">\n      <tr>\n        <th>ID</th>\n        <th>NAMES</th>\n        <th>COUNTRY</th>\n        <th>PROVINCE</th>\n        <th>DISTRICT</th>\n        <th>ACTION</th>\n      </tr>\n    </thead>\n    <tbody>\n      <tr v-for=\"(agent, index) in paginatedAgents\" :key=\"agent.users_id\">\n        <td>{{ (currentPage - 1) * perPage + index + 1 }}</td>\n        <td>{{ agent.first_name }} {{ agent.last_name }}</td>\n        <td>{{ agent.country }}</td>\n        <td>{{ agent.province }}</td>\n        <td>{{ agent.district }}</td>\n        <td>\n          <div class=\"d-flex gap-2\">\n            <button class=\"btn btn-primary btn-sm\" @click=\"goToEdit(agent.users_id)\" :disabled=\"!isAdmin\">Edit</button>\n            <button class=\"btn btn-cancel btn-sm\" @click=\"goToMore(agent.users_id)\">More</button>\n            <button class=\"btn btn-primary btn-sm\" @click=\"confirmDelete(agent.users_id)\" :disabled=\"!isAdmin\">Delete</button>\n          </div>\n        </td>\n      </tr>\n    </tbody>\n  </table>\n</div>\n\n\n      <!-- Pagination -->\n      <div v-if=\"totalPages > 1\" class=\"d-flex flex-column align-items-center my-3\">\n        <nav>\n          <ul class=\"pagination\">\n            <li :class=\"['page-item', { disabled: currentPage === 1 }]\">\n              <button class=\"page-link\" @click=\"changePage(currentPage - 1)\">Previous</button>\n            </li>\n            <li\n              v-for=\"page in totalPages\"\n              :key=\"page\"\n              :class=\"['page-item', { active: currentPage === page }]\"\n            >\n              <button class=\"page-link\" @click=\"changePage(page)\">{{ page }}</button>\n            </li>\n            <li :class=\"['page-item', { disabled: currentPage === totalPages }]\">\n              <button class=\"page-link\" @click=\"changePage(currentPage + 1)\">Next</button>\n            </li>\n          </ul>\n        </nav>\n      </div>\n    </div>\n  </div>\n\n  <!-- Modal -->\n  <div v-if=\"showModal\" class=\"modal-overlay\">\n    <div class=\"modal-content\">\n      <h5>Are you sure you want to delete this view agent content?</h5>\n      <div>\n        <button class=\"btn btn-primary mt-3\" @click=\"deleteAbout(selectedAboutId)\">Yes, Delete</button>\n        <button class=\"btn btn-cancel mt-3\" style=\"background-color: teal; color: white;\" @click=\"closeModal\">\n          Cancel\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport IndexComponent from \"./IndexComponent.vue\";\nimport { globalVariable } from \"@/global\";\n\nexport default {\n  name: \"MainWrapper\",\n  components: { IndexComponent },\n  data() {\n    return {\n      jobs: [],\n      searchQuery: \"\",\n      currentPage: 1,\n      perPage: 20,\n      userEmail: \"\",\n      userId: \"\",\n      showModal: false,\n      selectedAboutId: null,\n    };\n  },\n\n  computed: {\n    isAdmin() {\n    return this.userEmail.toLowerCase() === \"admin@kozi.rw\";\n  },\n    filteredAgents() {\n      const query = this.searchQuery.toLowerCase();\n      return this.jobs.filter((agent) => {\n        const name = `${agent.first_name} ${agent.last_name}`.toLowerCase();\n        const province = agent.province?.toLowerCase() || \"\";\n        const district = agent.district?.toLowerCase() || \"\";\n        return (\n          name.includes(query) || province.includes(query) || district.includes(query)\n        );\n      });\n    },\n\n    paginatedAgents() {\n      const start = (this.currentPage - 1) * this.perPage;\n      const end = start + this.perPage;\n      return this.filteredAgents.slice(start, end);\n    },\n\n    totalPages() {\n      return Math.ceil(this.filteredAgents.length / this.perPage);\n    },\n  },\n\n  watch: {\n    searchQuery() {\n      this.currentPage = 1;\n    },\n  },\n\n  mounted() {\n    this.userEmail = localStorage.getItem(\"userEmail\") || \"\";\n    if (this.userEmail) {\n      this.getUserIdFromEmail();\n    }\n  },\n\n  methods: {\n    changePage(page) {\n      if (page >= 1 && page <= this.totalPages) {\n        this.currentPage = page;\n      }\n    },\n\n    async fetchJobsByUser() {\n      try {\n        const token = localStorage.getItem(\"adminToken\");\n        if (!this.userId) return;\n        const res = await fetch(`${globalVariable}/admin/select_agent`, {\n          headers: {\n            Authorization: `Bearer ${token}`,\n          },\n        });\n        if (!res.ok) throw new Error(\"Failed to fetch agents\");\n        const data = await res.json();\n        this.jobs = data;\n      } catch (err) {\n        console.error(\"Error fetching agents:\", err);\n      }\n    },\n\n    goToEdit(users_id) {\n      this.$router.push({ name: \"edit-agent\", params: { id: users_id } });\n    },\n\n    goToMore(users_id) {\n      this.$router.push({ name: \"more-agent\", params: { id: users_id } });\n    },\n\n    confirmDelete(id) {\n      this.selectedAboutId = id;\n      this.showModal = true;\n    },\n\n    closeModal() {\n      this.showModal = false;\n      this.selectedAboutId = null;\n    },\n\n    async deleteAbout(users_id) {\n      const token = localStorage.getItem(\"adminToken\");\n      try {\n        const res = await fetch(`${globalVariable}/admin/delete_agent/${users_id}`, {\n          method: \"DELETE\",\n          headers: {\n            Authorization: `Bearer ${token}`,\n          },\n        });\n        const data = await res.json();\n        if (res.ok) {\n          this.jobs = this.jobs.filter((job) => job.users_id !== users_id);\n        } else {\n          console.error(\"Failed to delete:\", data.message);\n        }\n      } catch (err) {\n        console.error(\"Error deleting agent:\", err);\n      } finally {\n        this.closeModal();\n      }\n    },\n\n    async getUserIdFromEmail() {\n      const token = localStorage.getItem(\"adminToken\");\n      if (!token) return;\n      try {\n        const payload = JSON.parse(atob(token.split(\".\")[1]));\n        if (!this.userEmail) {\n          this.userEmail = payload.email;\n        }\n        const res = await fetch(`${globalVariable}/get_user_id_by_email/${this.userEmail}`, {\n          headers: { Authorization: `Bearer ${token}` },\n        });\n        const data = await res.json();\n        if (res.ok) {\n          this.userId = data.users_id;\n          this.fetchJobsByUser();\n        } else {\n          console.error(\"Unable to get user ID:\", data.message);\n        }\n      } catch (err) {\n        console.error(\"Error getting user ID:\", err);\n      }\n    },\n  },\n};\n</script>\n\n\n\n\n<style scoped>\n.page-item.active .page-link {\n  background-color: #E960A6;\n  color: white;\n  border-color: #E960A6;\n}\n\n.page-link {\n  border-radius: 4px;\n  padding: 0.4rem 0.75rem;\n  cursor: pointer;\n  color:#336Cb6\n}\n\n      .modal-overlay {\n         position: fixed;\n          top: 0;\n          left: 0;\n          width: 100%;\n          height: 100%;\n          background: rgba(0, 0, 0, 0.5);\n          display: flex;\n          justify-content: center;\n          align-items: center;\n          z-index: 1000;\n        }\n        \n        /* Modal content styling */\n        .modal-content {\n          background-color: white;\n          padding: 20px;\n          border-radius: 8px;\n          text-align: center;\n          width: 300px;\n        }\n      \n        .container-fluid {\n          max-width: 800px;\n          margin: auto;\n          padding: 20px;\n          background-color: #ffffff;\n          border-radius: 8px;\n          box-shadow: 0 8px 30px rgba(0, 0, 0, 0.2);\n        }\n        \n        h5 {\n          color: #333;\n          margin-bottom: 20px;\n        }\n        .custom-select {\n          width: 100%;\n          padding: 15px;\n          height: 50px;\n          border: 1px solid #ccc;\n          border-radius: 5px;\n          box-sizing: border-box;\n          transition: border-color 0.3s, box-shadow 0.3s;\n          font-size: 16px;\n    \n        }\n        .form-control {\n          width: 100%;\n          padding: 15px;\n          height: 50px;\n          border: 1px solid #ccc;\n          border-radius: 5px;\n          box-sizing: border-box;\n          transition: border-color 0.3s, box-shadow 0.3s;\n          font-size: 16px;\n        }\n        \n        .form-control:focus {\n          border-color: #E960A6;\n          box-shadow: 0 0 5px rgba(0, 123, 255, 0.5);\n          outline: none;\n        }\n        \n        .form-label {\n          font-weight: bold;\n          margin-bottom: 5px;\n          display: block;\n        }\n        \n        .btn-primary {\n          background-color: #E960A6;\n          color: white;\n        \n        }\n        \n        .btn-primary:hover {\n          background-color: #E960A6;\n          color: teal;\n        }\n        .btn-cancel {\n          background-color: teal;\n          color: white;\n        \n        }\n        \n        .btn-cancel:hover {\n          background-color: teal ;\n          color: #E960A6;\n        }\n        \n        .form-group {\n          margin-bottom: 15px;\n        }\n        \n        </style>\n"],"mappings":";;;;AAyGA,OAAOA,cAAa,MAAO,sBAAsB;AACjD,SAASC,cAAa,QAAS,UAAU;AAEzC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IAAEH;EAAe,CAAC;EAC9BI,IAAIA,CAA