{"ast":null,"code":"import HeaderComponent from \"./HeaderComponent.vue\";\nimport SidebarComponent from \"./SidebarComponent.vue\";\nexport default {\n  components: {\n    HeaderComponent,\n    SidebarComponent\n  },\n  data() {\n    return {\n      sidebarVisible: false,\n      isMobile: window.innerWidth <= 768\n    };\n  },\n  mounted() {\n    window.addEventListener(\"resize\", this.checkMobile);\n    this.checkMobile(); // Ensure correct state on initial load\n  },\n  beforeUnmount() {\n    window.removeEventListener(\"resize\", this.checkMobile);\n  },\n  methods: {\n    toggleSidebar() {\n      this.sidebarVisible = !this.sidebarVisible;\n    },\n    checkMobile() {\n      this.isMobile = window.innerWidth <= 768;\n      if (!this.isMobile) {\n        this.sidebarVisible = true;\n      }\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","SidebarComponent","components","data","sidebarVisible","isMobile","window","innerWidth","mounted","addEventListener","checkMobile","beforeUnmount","removeEventListener","methods","toggleSidebar"],"sources":["/Users/gentil/Desktop/Kozi-ai-agent/src/views/agent/IndexComponent.vue"],"sourcesContent":["<template>\n  <div id=\"main-wrapper\">\n    <SidebarComponent\n      :visible=\"sidebarVisible\"\n      @close-sidebar=\"sidebarVisible = false\"\n    />\n\n    <div class=\"body-wrapper\" :class=\"{ hidden: isMobile && sidebarVisible }\">\n      <HeaderComponent @toggle-sidebar=\"toggleSidebar\" />\n      <slot />\n    </div>\n  </div>\n</template>\n\n<script>\nimport HeaderComponent from \"./HeaderComponent.vue\";\nimport SidebarComponent from \"./SidebarComponent.vue\";\n\nexport default {\n  components: { HeaderComponent, SidebarComponent },\n  data() {\n    return {\n      sidebarVisible: false,\n      isMobile: window.innerWidth <= 768,\n    };\n  },\n  mounted() {\n    window.addEventListener(\"resize\", this.checkMobile);\n    this.checkMobile(); // Ensure correct state on initial load\n  },\n  beforeUnmount() {\n    window.removeEventListener(\"resize\", this.checkMobile);\n  },\n  methods: {\n    toggleSidebar() {\n      this.sidebarVisible = !this.sidebarVisible;\n    },\n    checkMobile() {\n      this.isMobile = window.innerWidth <= 768;\n      if (!this.isMobile) {\n        this.sidebarVisible = true;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n#main-wrapper {\n  display: flex;\n  height: 100vh;\n  width: 100%;\n}\n\n.body-wrapper {\n  flex: 1;\n  transition: filter 0.3s ease;\n  width: 100%;\n  /* Remove padding that's causing the margins */\n}\n\n.body-wrapper.hidden {\n  filter: blur(3px);\n  pointer-events: none;\n  user-select: none;\n}\n\n@media (min-width: 769px) {\n  .body-wrapper {\n    margin-left: 270px;\n    width: calc(100% - 270px);\n  }\n}\n\n@media (max-width: 768px) {\n  .body-wrapper {\n    margin-left: 0;\n    width: 100%;\n  }\n  \n  /* Ensure header has no padding/margin on mobile */\n  .body-wrapper :deep(.app-header) {\n    margin: 0;\n    padding: 0;\n  }\n  \n  .body-wrapper :deep(.navbar) {\n    padding-left: 0.5rem !important;\n    padding-right: 0.5rem !important;\n  }\n}\n</style>"],"mappings":"AAeA,OAAOA,eAAc,MAAO,uBAAuB;AACnD,OAAOC,gBAAe,MAAO,wBAAwB;AAErD,eAAe;EACbC,UAAU,EAAE;IAAEF,eAAe;IAAEC;EAAiB,CAAC;EACjDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,KAAK;MACrBC,QAAQ,EAAEC,MAAM,CAACC,UAAS,IAAK;IACjC,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRF,MAAM,CAACG,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACC,WAAW,CAAC;IACnD,IAAI,CAACA,WAAW,CAAC,CAAC,EAAE;EACtB,CAAC;EACDC,aAAaA,CAAA,EAAG;IACdL,MAAM,CAACM,mBAAmB,CAAC,QAAQ,EAAE,IAAI,CAACF,WAAW,CAAC;EACxD,CAAC;EACDG,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd,IAAI,CAACV,cAAa,GAAI,CAAC,IAAI,CAACA,cAAc;IAC5C,CAAC;IACDM,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACL,QAAO,GAAIC,MAAM,CAACC,UAAS,IAAK,GAAG;MACxC,IAAI,CAAC,IAAI,CAACF,QAAQ,EAAE;QAClB,IAAI,CAACD,cAAa,GAAI,IAAI;MAC5B;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}