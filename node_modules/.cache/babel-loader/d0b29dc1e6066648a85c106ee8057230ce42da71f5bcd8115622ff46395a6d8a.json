{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n/* global google */\nimport axios from \"axios\";\nimport { globalVariable } from \"@/global\";\nexport default {\n  name: \"SignupPage\",\n  data() {\n    return {\n      email: \"\",\n      first_name: \"\",\n      last_name: \"\",\n      telephone: \"\",\n      password: \"\",\n      role_id: null,\n      loginInProgress: false,\n      message: \"\",\n      messageType: \"\",\n      googleClientId: \"793211149046-copaoe356k4gg5qv1bjmu92egvsn2fcn.apps.googleusercontent.com\",\n      agreedToTerms: false,\n      showPassword: false\n    };\n  },\n  mounted() {\n    const roleId = localStorage.getItem(\"selectedRoleId\");\n    if (!roleId) {\n      this.$router.push(\"/role-selector\");\n    } else {\n      this.role_id = roleId;\n      this.$nextTick(() => {\n        if (window.google) {\n          google.accounts.id.initialize({\n            client_id: this.googleClientId,\n            callback: this.handleGoogleCredential\n          });\n          google.accounts.id.renderButton(document.getElementById(\"google-button\"), {\n            theme: \"outline\",\n            size: \"large\",\n            width: \"100%\",\n            text: \"signup_with\"\n          });\n        }\n      });\n    }\n  },\n  computed: {\n    roleName() {\n      switch (parseInt(this.role_id)) {\n        case 1:\n          return \"Job Seeker\";\n        case 2:\n          return \"Employer\";\n        case 3:\n          return \"Admin\";\n        case 4:\n          return \"Agent\";\n        default:\n          return \"Unknown Role\";\n      }\n    },\n    roleDisplayName() {\n      switch (parseInt(this.role_id)) {\n        case 1:\n          return \"Job Seeker Hub\";\n        case 2:\n          return \"Employer Portal\";\n        case 3:\n          return \"Admin Dashboard\";\n        case 4:\n          return \"Agent Portal\";\n        default:\n          return \"JobPortal\";\n      }\n    },\n    roleSignupTagline() {\n      switch (parseInt(this.role_id)) {\n        case 1:\n          return {\n            line1: \"Start your career journey today\",\n            line2: \"Join thousands of successful professionals\"\n          };\n        case 2:\n          return {\n            line1: \"Build your dream team\",\n            line2: \"Connect with top talent effortlessly\"\n          };\n        case 3:\n          return {\n            line1: \"Gain complete control\",\n            line2: \"Manage platform operations with ease\"\n          };\n        case 4:\n          return {\n            line1: \"Become a career connector\",\n            line2: \"Help others find their perfect match\"\n          };\n        default:\n          return {\n            line1: \"Join our community\",\n            line2: \"Unlock new opportunities\"\n          };\n      }\n    },\n    roleFeatures() {\n      switch (parseInt(this.role_id)) {\n        case 1:\n          // Job Seeker\n          return {\n            feature1: {\n              icon: \"🔍\",\n              text: \"Search jobs\"\n            },\n            feature2: {\n              icon: \"📄\",\n              text: \"Build Resume\"\n            },\n            feature3: {\n              icon: \"🎯\",\n              text: \"Apply Smartly\"\n            }\n          };\n        case 2:\n          // Job Provider\n          return {\n            feature1: {\n              icon: \"📝\",\n              text: \"Post Jobs\"\n            },\n            feature2: {\n              icon: \"👥\",\n              text: \"Find Talent\"\n            },\n            feature3: {\n              icon: \"⚡\",\n              text: \"Fast and Secure Hiring\"\n            }\n          };\n        case 3:\n          // Admin\n          return {\n            feature1: {\n              icon: \"📊\",\n              text: \"Analytics\"\n            },\n            feature2: {\n              icon: \"⚙️\",\n              text: \"Settings\"\n            },\n            feature3: {\n              icon: \"🛡️\",\n              text: \"Security\"\n            }\n          };\n        case 4:\n          // Agent\n          return {\n            feature1: {\n              icon: \"🤝\",\n              text: \"Connect\"\n            },\n            feature2: {\n              icon: \"💼\",\n              text: \"Manage\"\n            },\n            feature3: {\n              icon: \"📈\",\n              text: \"Track\"\n            }\n          };\n        default:\n          return {\n            feature1: {\n              icon: \"💼\",\n              text: \"Find Jobs\"\n            },\n            feature2: {\n              icon: \"🚀\",\n              text: \"Grow Career\"\n            },\n            feature3: {\n              icon: \"🎯\",\n              text: \"Match Skills\"\n            }\n          };\n      }\n    }\n  },\n  methods: {\n    limitPhone(event) {\n      if (event.target.value.length > 12) {\n        event.target.value = event.target.value.slice(0, 12);\n        this.telephone = event.target.value;\n      }\n    },\n    async signup() {\n      if (!this.role_id) {\n        this.message = \"Please select a role before signing up.\";\n        this.messageType = \"alert-danger\";\n        return;\n      }\n      if (!this.agreedToTerms) {\n        this.message = \"You must agree to the Terms and Conditions.\";\n        this.messageType = \"alert-danger\";\n        return;\n      }\n      this.loginInProgress = true;\n      this.message = \"\";\n      try {\n        const endpoint = this.role_id == 2 ? `${globalVariable}/provider/signup` : `${globalVariable}/seeker/signup`;\n        const response = await axios.post(endpoint, {\n          email: this.email,\n          first_name: this.first_name,\n          last_name: this.last_name,\n          full_name: `${this.first_name} ${this.last_name}`,\n          telephone: this.telephone,\n          password: this.password,\n          role_id: this.role_id,\n          picture: \"\",\n          verifiedEmail: false,\n          token: \"\"\n        });\n        if (response.status === 200) {\n          localStorage.setItem(\"signupEmail\", this.email);\n          this.$router.push(\"/verify-otp\");\n        } else {\n          this.message = response.data.message || \"Signup failed.\";\n          this.messageType = \"alert-danger\";\n        }\n      } catch (error) {\n        this.message = error.response?.data?.message || \"Signup failed.\";\n        this.messageType = \"alert-danger\";\n      } finally {\n        this.loginInProgress = false;\n      }\n    },\n    async handleGoogleCredential(response) {\n      const credential = response.credential;\n      try {\n        const res = await axios.post(`${globalVariable}/google-login`, {\n          token: credential,\n          role_id: this.role_id\n        });\n        if (res.data.token) {\n          localStorage.setItem(\"authToken\", res.data.token);\n          localStorage.setItem(\"userEmail\", res.data.email);\n          localStorage.setItem(\"roleId\", this.role_id);\n          this.routeUser();\n        } else {\n          this.message = \"Google signup failed.\";\n          this.messageType = \"alert-danger\";\n        }\n      } catch (error) {\n        this.message = error.response?.data?.message || \"Google signup failed.\";\n        this.messageType = \"alert-danger\";\n      }\n    },\n    routeUser() {\n      switch (parseInt(this.role_id)) {\n        case 1:\n          this.$router.push(\"/dashboard\");\n          break;\n        case 2:\n          this.$router.push(\"/employer/dashboard\");\n          break;\n        case 3:\n          this.$router.push(\"/admin-dashboard\");\n          break;\n        case 4:\n          this.$router.push(\"/agent-dashboard\");\n          break;\n        default:\n          this.$router.push(\"/dashboard\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","globalVariable","name","data","email","first_name","last_name","telephone","password","role_id","loginInProgress","message","messageType","googleClientId","agreedToTerms","showPassword","mounted","roleId","localStorage","getItem","$router","push","$nextTick","window","google","accounts","id","initialize","client_id","callback","handleGoogleCredential","renderButton","document","getElementById","theme","size","width","text","computed","roleName","parseInt","roleDisplayName","roleSignupTagline","line1","line2","roleFeatures","feature1","icon","feature2","feature3","methods","limitPhone","event","target","value","length","slice","signup","endpoint","response","post","full_name","picture","verifiedEmail","token","status","setItem","error","credential","res","routeUser"],"sources":["/Users/gentil/Desktop/Kozi-ai-agent/src/views/SignUp.vue"],"sourcesContent":["<template>\n  <div class=\"page-wrapper\" id=\"main-wrapper\">\n    <div class=\"login-layout\">\n      <!-- Image Section -->\n      <div class=\"image-section\" style=\"background: linear-gradient(135deg, #EA60A7 0%, #d946a3 50%, #c026d3 100%);\">\n        <div class=\"image-overlay\">\n          <div class=\"brand-content\">\n            <div class=\"brand-logo\">\n              <div class=\"logo-icon\">\n                <svg width=\"40\" height=\"40\" viewBox=\"0 0 40 40\" fill=\"none\">\n                  <path d=\"M20 0C31.046 0 40 8.954 40 20C40 31.046 31.046 40 20 40C8.954 40 0 31.046 0 20C0 8.954 8.954 0 20 0Z\" fill=\"white\"/>\n                  <path d=\"M20 8C15.582 8 12 11.582 12 16V20C12 24.418 15.582 28 20 28C24.418 28 28 24.418 28 20V16C28 11.582 24.418 8 20 8Z\" fill=\"#EA60A7\"/>\n                </svg>\n              </div>\n              <h1 class=\"logo-text\">{{ roleDisplayName }}</h1>\n            </div>\n            <div class=\"brand-tagline\">\n              <p>{{ roleSignupTagline.line1 }}</p>\n              <p>{{ roleSignupTagline.line2 }}</p>\n            </div>\n          </div>\n          <div class=\"floating-elements\">\n            <div class=\"floating-card card-1\">\n              <div class=\"card-icon\">{{ roleFeatures.feature1.icon }}</div>\n              <span>{{ roleFeatures.feature1.text }}</span>\n            </div>\n            <div class=\"floating-card card-2\">\n              <div class=\"card-icon\">{{ roleFeatures.feature2.icon }}</div>\n              <span>{{ roleFeatures.feature2.text }}</span>\n            </div>\n            <div class=\"floating-card card-3\">\n              <div class=\"card-icon\">{{ roleFeatures.feature3.icon }}</div>\n              <span>{{ roleFeatures.feature3.text }}</span>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Form Section -->\n      <div class=\"form-section\">\n        <div class=\"form-container\">\n          <div class=\"form-header\">\n            <h2 class=\"form-title\">Join {{ roleName }}</h2>\n            <p class=\"form-subtitle\">Create your {{ roleName.toLowerCase() }} account</p>\n            <p style=\"margin-top: 2rem;\" class=\"form-label\">usanzwe ufite konti? <a href=\"/login\" class=\"link\">Kanda hano</a></p>\n             \n          </div>\n          \n\n          <form @submit.prevent=\"signup\" class=\"login-form\">\n            <div class=\"error-container\" v-if=\"message\">\n              <div class=\"alert\" :class=\"messageType\">\n                <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\">\n                  <path d=\"M8 0C3.58 0 0 3.58 0 8C0 12.42 3.58 16 8 16C12.42 16 16 12.42 16 8C16 3.58 12.42 0 8 0ZM9 12H7V10H9V12ZM9 8H7V4H9V8Z\" fill=\"currentColor\"/>\n                </svg>\n                {{ message }}\n              </div>\n            </div>\n\n            \n\n            <div class=\"form-row\">\n              <div class=\"form-group\">\n                <label for=\"first_name\" class=\"form-label\">Izina ribanza</label>\n                <div class=\"input-wrapper\">\n                  <svg class=\"input-icon\" xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 448 512\" fill=\"currentColor\">\n                    <path d=\"M224 256A128 128 0 1 0 224 0a128 128 0 1 0 0 256zm-45.7 48C79.8 304 0 383.8 0 482.3C0 498.7 13.3 512 29.7 512l388.6 0c16.4 0 29.7-13.3 29.7-29.7C448 383.8 368.2 304 269.7 304l-91.4 0z\"/>\n                  </svg>\n                  <input \n                    type=\"text\" \n                    v-model=\"first_name\" \n                    class=\"form-control\" \n                    id=\"first_name\"\n                    required \n                    placeholder=\"First name\"\n                  />\n                </div>\n              </div>\n\n              <div class=\"form-group\">\n                <label for=\"last_name\" class=\"form-label\">Izina rya nyuma  </label>\n                <div class=\"input-wrapper\">\n                  <svg class=\"input-icon\" xmlns=\"http://www.w3.org/2000/svg\" width=\"20\" height=\"20\" viewBox=\"0 0 448 512\" fill=\"currentColor\">\n                    <path d=\"M224 256A128 128 0 1 0 224 0a128 128 0 1 0 0 256zm-45.7 48C79.8 304 0 383.8 0 482.3C0 498.7 13.3 512 29.7 512l388.6 0c16.4 0 29.7-13.3 29.7-29.7C448 383.8 368.2 304 269.7 304l-91.4 0z\"/>\n                  </svg>\n\n                  <input \n                    type=\"text\" \n                    v-model=\"last_name\" \n                    class=\"form-control\" \n                    id=\"last_name\"\n                    required \n                    placeholder=\"Last name\"\n                  />\n                </div>\n              </div>\n            </div>\n\n            <div class=\"form-group\">\n              <label for=\"telephone\" class=\"form-label\">Nimero ya Telefoni</label>\n              <div class=\"input-wrapper\">\n                <svg class=\"input-icon\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 512 512\"   width=\"20\" height=\"20\" fill=\"currentColor\">\n                  <path d=\"M164.9 24.6c-7.7-18.6-28-28.5-47.4-23.2l-88 24C12.1 30.2 0 46 0 64C0 311.4 200.6 512 448 512c18 0 33.8-12.1 38.6-29.5l24-88c5.3-19.4-4.6-39.7-23.2-47.4l-96-40c-16.3-6.8-35.2-2.1-46.3 11.6L304.7 368C234.3 334.7 177.3 277.7 144 207.3L193.3 167c13.7-11.2 18.4-30 11.6-46.3l-40-96z\"/>\n                </svg>\n                \n                <input \n                type=\"number\" \n                v-model=\"telephone\" \n                class=\"form-control\" \n                id=\"telephone\"\n                required \n                placeholder=\"Enter your phone number\"\n                @input=\"limitPhone\"\n                />\n\n              </div>\n            </div>\n            <div class=\"form-group\">\n              <label for=\"email\" class=\"form-label\">Emeyili Yawe</label>\n              <div class=\"input-wrapper\">\n                <svg class=\"input-icon\" width=\"20\" height=\"20\" viewBox=\"0 0 20 20\" fill=\"none\">\n                  <path d=\"M18 4H2C1.45 4 1 4.45 1 5V15C1 15.55 1.45 16 2 16H18C18.55 16 19 15.55 19 15V5C19 4.45 18.55 4 18 4ZM18 7L10 12L2 7V5L10 10L18 5V7Z\" fill=\"currentColor\"/>\n                </svg>\n                <input \n                  type=\"email\" \n                  v-model=\"email\" \n                  class=\"form-control\" \n                  id=\"email\"\n                  required \n                  placeholder=\"Enter your email\"\n                />\n              </div>\n            </div>\n\n            <div class=\"form-group\">\n              <label for=\"password\" class=\"form-label\">Ijambo Banga</label>\n              <div class=\"input-wrapper\">\n                <svg class=\"input-icon\" width=\"20\" height=\"20\" viewBox=\"0 0 20 20\" fill=\"none\">\n                  <path d=\"M15 7H14V5C14 2.24 11.76 0 9 0S4 2.24 4 5V7H3C2.45 7 2 7.45 2 8V18C2 18.55 2.45 19 3 19H15C15.55 19 16 18.55 16 18V8C16 7.45 15.55 7 15 7ZM6 5C6 3.34 7.34 2 9 2S12 3.34 12 5V7H6V5Z\" fill=\"currentColor\"/>\n                </svg>\n                <input\n                  class=\"form-control\"\n                  v-model=\"password\"\n                  :type=\"showPassword ? 'text' : 'password'\"\n                  id=\"password\"\n                  required\n                  placeholder=\"Create a strong password\"\n                />\n                <button \n                  type=\"button\" \n                  class=\"password-toggle\"\n                  @click=\"showPassword = !showPassword\"\n                >\n                  <svg v-if=\"showPassword\" width=\"20\" height=\"20\" viewBox=\"0 0 20 20\" fill=\"none\">\n                    <path d=\"M10 12C8.9 12 8 11.1 8 10S8.9 8 10 8S12 8.9 12 10S11.1 12 10 12ZM10 4C5 4 1.73 7.11 1 10C1.73 12.89 5 16 10 16S18.27 12.89 19 10C18.27 7.11 15 4 10 4ZM10 14C7.24 14 4.91 12.36 4 10C4.91 7.64 7.24 6 10 6S15.09 7.64 16 10C15.09 12.36 12.76 14 10 14Z\" fill=\"currentColor\"/>\n                  </svg>\n                  <svg v-else width=\"20\" height=\"20\" viewBox=\"0 0 20 20\" fill=\"none\">\n                    <path d=\"M10 6C12.76 6 15.09 7.64 16 10C15.09 12.36 12.76 14 10 14C7.24 14 4.91 12.36 4 10C4.91 7.64 7.24 6 10 6ZM10 4C5 4 1.73 7.11 1 10C1.73 12.89 5 16 10 16S18.27 12.89 19 10C18.27 7.11 15 4 10 4ZM10 8C8.9 8 8 8.9 8 10S8.9 12 10 12S12 11.1 12 10S11.1 8 10 8Z\" fill=\"currentColor\"/>\n                    <path d=\"M2 2L18 18\" stroke=\"currentColor\" stroke-width=\"2\"/>\n                  </svg>\n                </button>\n              </div>\n            </div>\n\n            <div class=\"form-check\">\n              <input \n                class=\"form-check-input\" \n                type=\"checkbox\" \n                id=\"termsCheck\"\n                v-model=\"agreedToTerms\"\n                required\n              />\n              <label class=\"form-check-label\" for=\"termsCheck\">\n                Soma kandi wemeze \n                <a href=\"https://kozi.rw/terms-of-service\" target=\"_blank\" rel=\"noopener noreferrer\" class=\"terms-link\">\n                  amategeko n'amabwiriza.\n                </a>\n              </label>\n            </div>\n\n            \n            \n            <button type=\"submit\" class=\"btn-primary\" :disabled=\"loginInProgress\">\n              <span v-if=\"loginInProgress\" class=\"spinner\"></span>\n              <svg v-else width=\"20\" height=\"20\" viewBox=\"0 0 20 20\" fill=\"none\">\n                <path d=\"M10 0C4.48 0 0 4.48 0 10S4.48 20 10 20S20 15.52 20 10S15.52 0 10 0ZM8 15L3 10L4.41 8.59L8 12.17L15.59 4.58L17 6L8 15Z\" fill=\"currentColor\"/>\n              </svg>\n              {{ loginInProgress ? 'Signing In...' : 'Injira' }}\n            </button>\n<!-- continue with google-->\n            <!-- <div class=\"divider\">\n              <span class=\"divider-text\">or continue with</span>\n            </div> -->\n\n            <!-- <div class=\"google-signin-container\">\n              <div id=\"google-button\"></div>\n            </div> -->\n\n            <div class=\"footer-links\">\n             \n              <span class=\"separator\">•</span>\n              <a href=\"/\" class=\"link\">\n                <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\">\n                  <path d=\"M8 0L1 7H4V16H12V7H15L8 0Z\" fill=\"currentColor\"/>\n                </svg>\n                Subira inyuma\n              </a>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n/* global google */\nimport axios from \"axios\";\nimport { globalVariable } from \"@/global\";\n\nexport default {\n  name: \"SignupPage\",\n  data() {\n    return {\n      email: \"\",\n      first_name: \"\",\n      last_name: \"\",\n      telephone: \"\",\n      password: \"\",\n      role_id: null,\n      loginInProgress: false,\n      message: \"\",\n      messageType: \"\",\n      googleClientId: \"793211149046-copaoe356k4gg5qv1bjmu92egvsn2fcn.apps.googleusercontent.com\",\n      agreedToTerms: false,\n      showPassword: false,\n    };\n  },\n  mounted() {\n    const roleId = localStorage.getItem(\"selectedRoleId\");\n\n    if (!roleId) {\n      this.$router.push(\"/role-selector\");\n    } else {\n      this.role_id = roleId;\n\n      this.$nextTick(() => {\n        if (window.google) {\n          google.accounts.id.initialize({\n            client_id: this.googleClientId,\n            callback: this.handleGoogleCredential,\n          });\n\n          google.accounts.id.renderButton(\n            document.getElementById(\"google-button\"),\n            { \n              theme: \"outline\", \n              size: \"large\",\n              width: \"100%\",\n              text: \"signup_with\"\n            }\n          );\n        }\n      });\n    }\n  },\n  computed: {\n    roleName() {\n      switch (parseInt(this.role_id)) {\n        case 1: return \"Job Seeker\";\n        case 2: return \"Employer\";\n        case 3: return \"Admin\";\n        case 4: return \"Agent\";\n        default: return \"Unknown Role\";\n      }\n    },\n    roleDisplayName() {\n      switch (parseInt(this.role_id)) {\n        case 1: return \"Job Seeker Hub\";\n        case 2: return \"Employer Portal\";\n        case 3: return \"Admin Dashboard\";\n        case 4: return \"Agent Portal\";\n        default: return \"JobPortal\";\n      }\n    },\n    roleSignupTagline() {\n      switch (parseInt(this.role_id)) {\n        case 1: \n          return {\n            line1: \"Start your career journey today\",\n            line2: \"Join thousands of successful professionals\"\n          };\n        case 2:\n          return {\n            line1: \"Build your dream team\",\n            line2: \"Connect with top talent effortlessly\"\n          };\n        case 3:\n          return {\n            line1: \"Gain complete control\",\n            line2: \"Manage platform operations with ease\"\n          };\n        case 4:\n          return {\n            line1: \"Become a career connector\",\n            line2: \"Help others find their perfect match\"\n          };\n        default:\n          return {\n            line1: \"Join our community\",\n            line2: \"Unlock new opportunities\"\n          };\n      }\n    },\n    roleFeatures() {\n      switch (parseInt(this.role_id)) {\n        case 1: // Job Seeker\n          return {\n            feature1: { icon: \"🔍\", text: \"Search jobs\" },\n            feature2: { icon: \"📄\", text: \"Build Resume\" },\n            feature3: { icon: \"🎯\", text: \"Apply Smartly\" }\n          };\n        case 2: // Job Provider\n          return {\n            feature1: { icon: \"📝\", text: \"Post Jobs\" },\n            feature2: { icon: \"👥\", text: \"Find Talent\" },\n            feature3: { icon: \"⚡\", text: \"Fast and Secure Hiring\" }\n          };\n        case 3: // Admin\n          return {\n            feature1: { icon: \"📊\", text: \"Analytics\" },\n            feature2: { icon: \"⚙️\", text: \"Settings\" },\n            feature3: { icon: \"🛡️\", text: \"Security\" }\n          };\n        case 4: // Agent\n          return {\n            feature1: { icon: \"🤝\", text: \"Connect\" },\n            feature2: { icon: \"💼\", text: \"Manage\" },\n            feature3: { icon: \"📈\", text: \"Track\" }\n          };\n        default:\n          return {\n            feature1: { icon: \"💼\", text: \"Find Jobs\" },\n            feature2: { icon: \"🚀\", text: \"Grow Career\" },\n            feature3: { icon: \"🎯\", text: \"Match Skills\" }\n          };\n      }\n    }\n  },\n  methods: {\n\n    limitPhone(event) {\n    if (event.target.value.length > 12) {\n      event.target.value = event.target.value.slice(0, 12);\n      this.telephone = event.target.value;\n    }\n  },\n    async signup() {\n      if (!this.role_id) {\n        this.message = \"Please select a role before signing up.\";\n        this.messageType = \"alert-danger\";\n        return;\n      }\n      if (!this.agreedToTerms) {\n        this.message = \"You must agree to the Terms and Conditions.\";\n        this.messageType = \"alert-danger\";\n        return;\n      }\n\n      this.loginInProgress = true;\n      this.message = \"\";\n\n      try {\n        const endpoint =\n          this.role_id == 2\n            ? `${globalVariable}/provider/signup`\n            : `${globalVariable}/seeker/signup`;\n\n        const response = await axios.post(endpoint, {\n          email: this.email,\n          first_name: this.first_name,\n          last_name: this.last_name,\n          full_name: `${this.first_name} ${this.last_name}`,\n          telephone: this.telephone,\n          password: this.password,\n          role_id: this.role_id,\n          picture: \"\",\n          verifiedEmail: false,\n          token: \"\"\n        });\n\n        if (response.status === 200) {\n          localStorage.setItem(\"signupEmail\", this.email);\n          this.$router.push(\"/verify-otp\");\n        } else {\n          this.message = response.data.message || \"Signup failed.\";\n          this.messageType = \"alert-danger\";\n        }\n      } catch (error) {\n        this.message = error.response?.data?.message || \"Signup failed.\";\n        this.messageType = \"alert-danger\";\n      } finally {\n        this.loginInProgress = false;\n      }\n    },\n\n    async handleGoogleCredential(response) {\n      const credential = response.credential;\n\n      try {\n        const res = await axios.post(`${globalVariable}/google-login`, {\n          token: credential,\n          role_id: this.role_id\n        });\n\n        if (res.data.token) {\n          localStorage.setItem(\"authToken\", res.data.token);\n          localStorage.setItem(\"userEmail\", res.data.email);\n          localStorage.setItem(\"roleId\", this.role_id);\n          this.routeUser();\n        } else {\n          this.message = \"Google signup failed.\";\n          this.messageType = \"alert-danger\";\n        }\n      } catch (error) {\n        this.message =